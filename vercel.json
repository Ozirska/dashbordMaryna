{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "app.js"
    }
  ],
  "public": [
    {
      "src": "/styles.css",
      "use": "@vercel/static"
    }
  ],
  "env": {
    "VIEWS_PATH": "./views"
  }
}
